{
	"version": "0.1.0",
	"configurations": [
		{
			"name": "Launch localhost",
			"type": "chrome",
			"request": "launch",
			"url": "http://localhost:8000/",
			"webRoot": "${workspaceFolder}/index.html"
		},
		{
			"name": "Launch index.html (disable sourcemaps)",
			"type": "chrome",
			"request": "launch",
			"sourceMaps": false,
			"file": "${workspaceFolder}/index.html"
		},
	]
}